{
  "env": {
    "browser": true
  },
  "extends": [
    "airbnb",
    "plugin:vue/recommended",
    "prettier"
  ],
  "globals": {
    "Vue": true,
    "chrome": true,
    "document": true,
    "window": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "plugins": [
    "vue",
    "html",
    "flowtype-errors"
  ],
  "rules": {
    "linebreak-style": "off",
    "no-console": "off",
    "flowtype-errors/show-errors": 2
  }
}